image: {{ edpm_nova_libvirt_container_image }}
net: none
privileged: false
user: root
volumes:
  - /var/log/containers/libvirt/swtpm:/var/log/swtpm:shared,z
command: ['/bin/bash', '-c', 'chown -R tss:tss /var/log/swtpm']
environment:
  EDPM_DEPLOY_IDENTIFIER: "{{ edpm_deploy_identifier | default('') }}"
